record(bi,"VME$(CRATE):$(BOARD):power_ok_RBV")
{
field(DTYP,"asynUInt32Digital")
field(SCAN,"1 second")
#  Source string : 0:fail;1:OK
	field(ZNAM,"fail")	field(ONAM,"OK")	
field(DESC,"overall power sensor")
field(INP,"@asynMask($(BOARD),0,0x00000001,1)VME_MON_STATUS")
}
