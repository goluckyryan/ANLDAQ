record(mbbo,"VME$(CRATE):$(BOARD):clk_select")
{
field(DTYP,"asynUInt32Digital")
#  Source string : 0:S/D:0;1:OSC:1;2:S/D:2;3:AUX:3;
	field(ZRST,"S/D")	field(ZRVL,"0")	field(ONST,"OSC")	field(ONVL,"1")	field(TWST,"S/D")	field(TWVL,"2")	field(THST,"AUX")	field(THVL,"3")	
field(DESC,"clock selection")
field(OUT,"@asynMask($(BOARD),0,0x00000003,1)vme_clk_ctrl")
}
